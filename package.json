{
  "name": "twr-wasm",
  "version": "2.5.0",
  "description": "twr-wasm provides a simple way to run C/C++ code in a web browser using WebAssembly.",
  "keywords": [
    "wasm",
    "webassembly",
    "typescript",
    "javascript",
    "emscripten",
    "hello world",
    "learn",
    "tutorial",
	 "walkthrough",
	 "api",
    "canvas",
    "audio",
	 "2d",
	 "draw",
    "example",
    "maze",
    "pong",
    "fft",
    "console",
	 "stdio",
	 "terminal",
    "crt",
    "standard C library",
    "easy",
    "simple",
    "light",
    "lightweight",
    "win32",
    "C/C++",
    "libc++"
  ],
  "files": [
    "./examples/*/*.html",
    "./examples/*/*.c",
    "./examples/*/*.cpp",
    "./examples/*/*.h",
    "./examples/*/*.ts",
    "./examples/*/*.js",
    "./examples/*/*.mp3",
    "./examples/*/*.jpg",
    "./examples/*/out/*.js",
    "./examples/*/*.txt",
    "./examples/*/*.wasm",
    "./examples/*/package.json",
    "./examples/*/tsconfig.json",
    "./examples/*/Makefile*",
    "./examples/*",
    "./include/**",
    "./lib-c/*",
    "./lib-js/*",
    "./LICENSE",
    "./readme*"
  ],
  "module": "./lib-js/index.js",
  "exports": {
    ".": "./lib-js/index.js",
    "./twrWasmModule": "./lib-js/twrmod.js",
    "./twrWasmModuleAsync": "./lib-js/twrmodasync.js"
  },
  "contributors": [
    {
      "name": "Anthony Wood",
      "email": "ajwood1965@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twiddlingbits/twr-wasm.git"
  },
  "license": "MIT",
  "homepage": "https://twiddlingbits.dev/"
}
