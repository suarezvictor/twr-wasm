{
  "name": "twr-wasm",
  "version": "2.3.0",
  "description": "twr-wasm provides a simple way to run C/C++ code in a web browser using WebAssembly.",
  "keywords": [
    "wasm",
    "webassembly",
    "hello world",
    "example",
	 "learn",
	 "tutorial",
	 "canvas",
	 "maze",
	 "pong",
    "crt",
    "standard C library",
    "light",
    "lightweight",
    "easy",
    "simple",
    "fft",
    "win32",
    "C/C++",
    "libc++",
    "typescript",
	 "javascript",
    "emscripten"
  ],
  "files": [
    "./examples/*/*.html",
    "./examples/*/*.c",
    "./examples/*/*.cpp",
    "./examples/*/*.h",
    "./examples/*/*.ts",
    "./examples/*/*.js",
    "./examples/*/*.txt",
    "./examples/*/*.wasm",
    "./examples/*/package.json",
    "./examples/*/tsconfig.json",
    "./examples/*/Makefile*",
    "./examples/*",
    "./include/**",
    "./lib-c/*",
    "./lib-js/*",
    "./LICENSE",
    "./readme*"
  ],
  "module": "./lib-js/index.js",
  "exports": {
    ".": "./lib-js/index.js",
    "./twrWasmModule": "./lib-js/twrmod.js",
    "./twrWasmModuleAsync": "./lib-js/twrmodasync.js"
  },
  "contributors": [
    {
      "name": "Anthony Wood",
      "email": "ajwood1965@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twiddlingbits/twr-wasm.git"
  },
  "license": "MIT",
  "homepage": "https://twiddlingbits.dev/"
}
