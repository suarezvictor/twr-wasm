{
    "configurations": [
      {
         "name": "Examples/Tests (Chrome)",
         "type": "chrome",
         "request": "launch",
         "runtimeArgs": [
               "--allow-file-access-from-files",
               "--autoplay-policy=no-user-gesture-required",
               "--enable-features=SharedArrayBuffer"
         ],
         "file": "${workspaceFolder}/examples/index.html",
         "cwd": "${workspaceFolder}/examples/"
      },
   ],
    "version": "2.0.0"
}